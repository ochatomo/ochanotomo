{
  "expo": {
    "name": "御茶ノ友",
    "slug": "ochanotomo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ochanotomo-icon.png",
    "splash": {
      "image": "./assets/ochanotomo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "sdkVersion": "42.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "bundleIdentifier": "com.SilverLining.ochanotomo",
      "supportsTablet": true
    },
    "android": {
      "package": "com.SilverLining.ochanotomo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ochanotomo-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
